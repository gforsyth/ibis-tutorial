{
  "postStartCommand": "bash",
  "dockerComposeFile": "compose.yaml",
  "service": "ibis-tutorial",
  "runServices": ["postgres"],
  "forwardPorts": ["postgres:5432"],
  "customizations": {
    "codespaces": {
      "openFiles": ["00 - Start Here.qmd"]
    },
    "vscode": {
      "extensions": ["ms-toolsai.jupyter", "ms-python.python", "quarto.quarto"]
    }
  },
  "features": {
    "ghcr.io/eitsupi/devcontainer-features/duckdb-cli:1": {
      "extensions": "httpfs,sqlite,postgres,parquet,json,arrow",
      "version": "0.10.2"
    },
    "ghcr.io/rocker-org/devcontainer-features/quarto-cli:1": {
      "version": "1.5.13"
    }
  }
}
